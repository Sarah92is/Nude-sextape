{
  "buildCommand": "npm install",
  "functions": {
    "api/signup.js": {
      "memory": 1024,
      "maxDuration": 10
    },
    "api/login.js": {
      "memory": 1024,
      "maxDuration": 10
    },
    "api/videos.js": {
      "memory": 1024,
      "maxDuration": 10
    }
  },
  "routes": [
    {
      "src": "/api/signup",
      "dest": "/api/signup.js"
    },
    {
      "src": "/api/login",
      "dest": "/api/login.js"
    },
    {
      "src": "/api/videos",
      "dest": "/api/videos.js"
    },
    {
      "src": "/videos/(.*)",
      "dest": "/public/videos/$1",
      "headers": {
        "Cache-Control": "public, max-age=3600"
      }
    },
    {
      "src": "/(.*)",
      "dest": "/public/index.html"
    }
  ],
  "public": "public"
}
